  "scripts": {
    "react-start": "yarn contract-build && react-app-rewired start",
    "react-build": "yarn contract-build && react-app-rewired build",
    "react-eject": "react-app-rewired eject",
    "react-test": "yarn contract-test && ts-mocha --exit --recursive --extension js,ts src/test",
    "contract-types": "yarn contract-build && typechain --out-dir types --target=truffle-v5 build/contracts/*.json",
    "contract-develop": "cd contracts && truffle develop --log --network development",
    "contract-migrate": "cd contracts && truffle migrate --reset --network development",
    "contract-test": "cd contracts && truffle test --network development",
    "contract-watch": "yarn test --watch",
    "contract-remix": "remixd -s contracts --remix-ide https://remix.ethereum.org",
    "contract-build": "cd contracts && truffle compile && typechain --target web3-v1 --out-dir build/typechain build/contracts/*.json",
    "contract-console": "cd contracts && truffle console"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-app-rewired": "^2.1.8",
    "wasm-loader": "^1.3.0"
  }

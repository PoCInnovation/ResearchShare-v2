{
  "name": "research-share-contracts",
  "version": "0.0.0",
  "main": "index.js",
  "author": "Ersikan <julien.philippon@epitech.eu>",
  "license": "MIT",
  "scripts": {
    "generate-types": "truffle compile; typechain --target=truffle-v5 'build/contracts/*.json'",
    "postinstall": "truffle compile; yarn generate-types",
    "develop": "truffle develop --network development",
    "migrate": "truffle migrate --reset",
    "test": "truffle test",
    "watch": "yarn test --watch",
    "remix": "remixd -s $(pwd) --remix-ide https://remix.ethereum.org",
    "build": "truffle compile; typechain --target web3-v1 --out-dir ./build/typechain ./build/contracts/*.json",
    "console": "truffle console"
  },
  "dependencies": {
    "@typechain/truffle-v5": "^5.1.0",
    "@typechain/web3-v1": "^3.1.0",
    "chai-bignumber": "^3.0.0",
    "chai-bn": "^0.3.0",
    "ts-node": "^10.2.1",
    "typechain": "^5.1.2"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^4.3.1",
    "@openzeppelin/test-environment": "^0.1.9",
    "@openzeppelin/test-helpers": "^0.5.13",
    "@remix-project/remixd": "^0.3.5",
    "@types/chai": "^4.2.18",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^8.2.2",
    "chai": "^4.3.4",
    "ganache-cli": "^6.12.2",
    "mocha": "^8.3.2",
    "truffle": "^5.3.3",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.3.2",
    "zokrates-js": "^1.0.32"
  }
}
